    APPEND l_edit_field TO lit_edit_fields.

    l_edit_field = &apos;IC_METHOD_COMBO&apos;.
    APPEND l_edit_field TO lit_edit_fields.

    l_edit_field = &apos;IC_PARAM_COMBO&apos;.
    APPEND l_edit_field TO lit_edit_fields.

