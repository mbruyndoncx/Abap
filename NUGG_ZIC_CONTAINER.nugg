    lo_ic_param-&gt;set_ic_value( lo_data ).

    lo_comp ?= lo_data.
    zcl_ic_persist=&gt;add( CHANGING ch_ic_comp = lo_comp ).

    zcl_ic_persist=&gt;save( lo_ic_param ).

*   Test 1
    cl_aunit_assert=&gt;assert_equals(
