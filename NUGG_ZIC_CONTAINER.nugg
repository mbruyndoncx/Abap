    IF NOT l_struct-description IS INITIAL.
      m_description = l_struct-description.
    ENDIF.

    IF NOT l_struct-rank IS INITIAL.
      m_rank = l_struct-rank.
    ENDIF.

  ENDIF.

  init( ).
