  <inheritance CLSNAME="ZCL_IC_SETTER_METHOD" REFCLSNAME="ZCL_IC_METHOD" VERSION="1" STATE="1">
   <redefinition CLSNAME="ZCL_IC_SETTER_METHOD" REFCLSNAME="ZCL_IC_METHOD" VERSION="1" MTDNAME="BUILD_PARAM_BINDINGS" EXPOSURE="1"/>
