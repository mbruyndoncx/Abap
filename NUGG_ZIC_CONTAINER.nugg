    wire_constructor( ).
    wire_attribs( ).

    wire_methods( i_is_setter_wiring = abap_true ).
    wire_methods( ).

    zif_ic_wirable~m_is_wired = abap_true.
  ENDIF.

ENDMETHOD.</source>
